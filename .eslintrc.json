{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends":["eslint:recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": ["warn", { "allow": ["error"] }],
        "prefer-const":["warn",{ "destructuring":"all" }],
        "no-unused-vars":["warn",{ "args":"none","ignoreRestSiblings":true }],
        "quotes":["error","single"]
    }

}
